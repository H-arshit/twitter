<% if @like %>
noty({text: 'Tweet was Liked', type: 'alert'});


$('#react_<%= @tweet.id%>_1 img').attr('src',$('#react_<%= @tweet.id%>_1 img').attr('like_2.svg'));
<% else %>
noty({text: 'Tweet was Liked', type: 'alert'});

$('#react_<%= @tweet.id%>_1 img').attr('src',$('#react_<%= @tweet.id%>_1 img').attr("like_1.svg"));

<% end %>

